! Xft
Xft.dpi: 96
Xft.antialias: true
Xft.autohint: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! Rofi
rofi.font: Source Code Pro 12

! Urxvt
URxvt.perl-ext-common: default

URxvt*font: xft:Source Code Pro:size=12
URxvt.scrollBar: false
URxvt.cursorBlink: true
URxvt*depth: 32
URxvt*background: rgba:2700/2800/2200/f000
URxvt*.foreground: #f8f8f2
URxvt*.cursorColor: #f8f8f2
URxvt*.color0: #272822
URxvt*.color1: #f92672
URxvt*.color2: #a6e22e
URxvt*.color3: #f4bf75
URxvt*.color4: #66d9ef
URxvt*.color5: #ae81ff
URxvt*.color6: #a1efe4
URxvt*.color7: #f8f8f2
URxvt*.color8: #75715e
URxvt*.color9: #f92672
URxvt*.color10: #a6e22e
URxvt*.color11: #f4bf75
URxvt*.color12: #66d9ef
URxvt*.color13: #ae81ff
URxvt*.color14: #a1efe4
URxvt*.color15: #f9f8f5

URxvt.inputMethod: fcitx
URxvt.preeditType: OverTheSpot

urxvt*urlLauncher: firefox
